<view class="imt-calendar data-v-9b20f3fe"><view class="header-c data-v-9b20f3fe"><view class="time-btn data-v-9b20f3fe">{{displayTime}}</view><block wx:if="{{!isSignIn}}"><view class="signin-btn data-v-9b20f3fe" hover-class="hover" data-event-opts="{{[['tap',[['signIn',['$event']]]]]}}" bindtap="__e">立即签到</view></block><block wx:else><view class="signin-btn down data-v-9b20f3fe" hover-class="hover">已签到</view></block></view><view class="calendar-week-wrapper data-v-9b20f3fe"><block wx:for="{{week}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="calendar-week data-v-9b20f3fe">{{item}}</view></block></view><swiper class="swiper data-v-9b20f3fe" current="{{current}}" data-event-opts="{{[['change',[['e0',['$event']]]]]}}" bindchange="__e"><block wx:for="{{calendar}}" wx:for-item="list" wx:for-index="index" wx:key="index"><swiper-item class="swiper-item data-v-9b20f3fe"><view class="day-list data-v-9b20f3fe"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="key" wx:key="key"><view class="{{['day-c','data-v-9b20f3fe',(item.completed)?'completed':'']}}"><view class="day data-v-9b20f3fe">{{''+item.date+''}}</view><block wx:if="{{item.score}}"><view class="score data-v-9b20f3fe">{{'+'+item.score+''}}</view></block><block wx:else><view class="score data-v-9b20f3fe"></view></block></view></block></view></swiper-item></block></swiper></view>