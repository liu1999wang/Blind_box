.top {
  background: white;
  background-image: url(data:image/png;base64,);
  background-size: 100% auto;
  background-repeat: no-repeat;
  position: fixed;
  width: 100%;
  top: 0rpx;
  z-index: 10;
  padding-bottom: 10px;
}
.top .card {
  display: flex;
  width: 670rpx;
  height: 174rpx;
  background: #FFFFFF;
  box-shadow: 0px 14rpx 30rpx 0px rgba(243, 238, 220, 0.4);
  border-radius: 20rpx;
  margin: 20rpx auto;
  padding: 10rpx 40rpx;
  align-items: center;
  box-sizing: border-box;
}
.top .card .headimg {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin-right: 50rpx;
  border: 10rpx solid white;
  box-shadow: 0px 14px 30px 0px rgba(226, 222, 204, 0.4);
}
.top .card .right {
  flex-grow: 1;
}
.top .card .right .bar {
  display: flex;
  justify-content: space-between;
}
.top .card .right .bar .item {
  text-align: center;
}
.top .card .right .bar .item .number {
  font-size: 32rpx;
  font-weight: 800;
  color: #000000;
}
.top .card .right .bar .item .key {
  font-size: 24rpx;
  font-weight: 500;
  color: #C6C6C6;
}
.top .title-c {
  display: flex;
  align-items: center;
  padding-right: 30rpx;
}
.top .title-c .text {
  padding: 0 24rpx;
  margin: 0 30rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 34rpx;
  font-weight: 500;
  position: relative;
  flex-grow: 1;
}
.top .title-c .text:after {
  content: '';
  position: absolute;
  left: 0;
  top: 18rpx;
  width: 3px;
  height: 34rpx;
  background-color: #ffcc00;
}
.top .title-c .item {
  border: 1px solid #e1e1e1;
  border-radius: 50rpx;
  padding: 2rpx 20rpx;
  font-size: 26rpx;
  margin-left: 10rpx;
}
.top .title-c .item.actived {
  background: #ffcc00;
  border: 1px solid #ffcc00;
  color: white;
  font-weight: 500;
}
.list {
  margin: 0 24rpx;
  padding-bottom: 180rpx;
}
.list-item {
  display: flex;
  padding: 24rpx 0;
  border-bottom: 1px solid #f3f3f3;
}
.list-item .order-number {
  font-size: 26rpx;
  margin-top: 6rpx;
}
.list-item .date-time {
  font-size: 24rpx;
  margin-top: 6rpx;
}
.list-body {
  flex: 1;
}
.right {
  font-size: 40rpx;
}
.footer {
  position: fixed;
  bottom: 20rpx;
  left: 75rpx;
  width: 100%;
  display: flex;
  width: 600rpx;
}
.footer .footer-button {
  flex: 1;
  margin: 0 auto;
  margin-bottom: 10rpx;
  height: 86rpx;
  line-height: 86rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
  background-color: #ffcc00;
  box-shadow: 0 0 20rpx rgba(255, 220, 0, 0.6);
}
.footer .footer-button.last-btn {
  margin-left: 30rpx;
}
.footer .footer-button.bg-purple {
  box-shadow: 0 0 20rpx rgba(81, 81, 149, 0.6);
}
.footer .footer-button view {
  background-color: #000000;
  color: white;
}

