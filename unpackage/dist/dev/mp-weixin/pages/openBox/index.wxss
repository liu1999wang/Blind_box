}
.yaoyiyao image {
  width: 76rpx;
  height: 76rpx;
  -webkit-animation: yaoyiyao 1s;
          animation: yaoyiyao 1s;
  -webkit-animation-iteration-count: infinite;
          animation-iteration-count: infinite;
  -webkit-transform-origin: center;
          transform-origin: center;
}
.yaoyiyao text {
  color: #333333;
  font-size: 24rpx;
  font-weight: bold;
}
