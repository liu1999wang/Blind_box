@-webkit-keyframes box-animation {
0% {
    -webkit-transform: rotate(-2deg);
            transform: rotate(-2deg);
}
50% {
    -webkit-transform: rotate(2deg);
            transform: rotate(2deg);
}
100% {
    -webkit-transform: rotate(-2deg);
            transform: rotate(-2deg);
}
}
@keyframes box-animation {
0% {
    -webkit-transform: rotate(-2deg);
            transform: rotate(-2deg);
}
50% {
    -webkit-transform: rotate(2deg);
            transform: rotate(2deg);
}
100% {
    -webkit-transform: rotate(-2deg);
            transform: rotate(-2deg);
}
}
.box-img.move {
  -webkit-animation: moving 1s;
          animation: moving 1s;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
}
.box-img.move2 {
  -webkit-animation: moving2 0.6s;
          animation: moving2 0.6s;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
}
@-webkit-keyframes moving {
0% {
    left: 0;
    -webkit-transform: scale(1);
            transform: scale(1);
}
35% {
    left: 80rpx;
    -webkit-transform: scale(0.6);
            transform: scale(0.6);
}
100% {
    left: -800rpx;
    -webkit-transform: scale(0.6);
            transform: scale(0.6);
}
}
@keyframes moving {
0% {
    left: 0;
    -webkit-transform: scale(1);
            transform: scale(1);
}
35% {
    left: 80rpx;
    -webkit-transform: scale(0.6);
            transform: scale(0.6);
}
100% {
    left: -800rpx;
    -webkit-transform: scale(0.6);
            transform: scale(0.6);
}
}
@-webkit-keyframes moving2 {
0% {
    left: 0;
    top: -800rpx;
}
100% {
    left: 0;
    top: 0;
}
}
@keyframes moving2 {
0% {
    left: 0;
    top: -800rpx;
}
100% {
    left: 0;
    top: 0;
}
}
