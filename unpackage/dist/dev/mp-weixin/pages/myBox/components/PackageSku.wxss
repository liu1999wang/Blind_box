.border-bottom-2 {
  border-bottom: 1px dashed #f1f1f1;
}
.lock-btn {
  background: #E8E8E8;
  border-radius: 50rpx;
  text-align: center;
  width: 80rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-weight: 500;
}
.order-item {
  background-color: #fff;
  margin: 30rpx 0;
  box-shadow: 0px 6rpx 30rpx 0px rgba(226, 222, 204, 0.5);
  border-radius: 20rpx;
}
.order-item .order-item-header {
  height: 70rpx;
  padding-left: 24rpx;
  display: flex;
  align-items: center;
  position: relative;
  font-size: 24rpx;
}
.order-item .order-item-header .time {
  font-size: 26rpx;
  font-weight: bold;
  color: #000000;
  line-height: 40rpx;
}
.order-item .order-item-header .node-type {
  background: #FCF6D8;
  color: #F58348;
  padding: 4rpx 10rpx;
  font-size: 24rpx;
  border-radius: 6rpx;
  margin-left: 10rpx;
}
.order-item .order-item-header .shang-title {
  background: #FCF6D8;
  color: #F58348;
  padding: 4rpx 10rpx;
  font-size: 24rpx;
  border-radius: 6rpx;
  margin-left: 10rpx;
}
.order-item .order-item-header .status {
  height: 100%;
  padding: 0rpx 26rpx 0rpx 44rpx;
  line-height: 50rpx;
  box-sizing: border-box;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position-x: right;
  color: white;
}
.order-item .order-item-header .status.working, .order-item .order-item-header .status.pending {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position-x: right;
  color: white;
}
.order-item .order-item-header .status.completed, .order-item .order-item-header .status.resaled, .order-item .order-item-header .status.exchanged, .order-item .order-item-header .status.comment_pending, .order-item .order-item-header .status.picked {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position-x: right;
  color: white;
}
.order-item .order-item-header .status.expired, .order-item .order-item-header .status.closed, .order-item .order-item-header .status.return_saled {
  color: #999999;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position-x: right;
}
.order-item .total-price {
  margin: 0rpx 24rpx;
  padding: 0 0rpx;
  height: 66rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.order-item .total-price .remark {
  font-size: 26rpx;
}
.order-item .total-price .price-number {
  display: flex;
  align-items: center;
  font-size: 24rpx;
}
.order-item .total-price .price-key {
  color: #999;
  font-weight: 500;
}
.order-item .total-price .price {
  margin-left: 8rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #FF5555;
  line-height: 26rpx;
}
.order-item .close-time {
  font-weight: 500;
  padding: 0 24rpx;
  font-size: 26rpx;
  text-align: right;
  margin-bottom: 10rpx;
}
.order-item .presell-c .tag {
  background: #FCF6D8;
  color: #F58348;
  padding: 4rpx 10rpx;
  font-size: 24rpx;
  border-radius: 6rpx;
}
.order-item .presell-c .date {
  font-size: 26rpx;
  margin-left: 6rpx;
}
.order-item .bottom-actions {
  padding: 10rpx 24rpx 18rpx 24rpx;
  display: flex;
  align-items: center;
}
.order-item .bottom-actions .check-box {
  flex-grow: 1;
  display: flex;
  align-content: center;
}
.order-item .bottom-actions .check-box text {
  font-size: 28rpx;
  margin-left: 2rpx;
}
.order-item .bottom-actions button {
  padding: 0;
  margin: 0;
  margin-left: 20rpx;
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  border-radius: 1000px;
  position: relative;
}
.order-item .bottom-actions button.bg-white {
  box-shadow: 0 2rpx 8rpx #cfcfcf;
}
.order-item .bottom-actions button:after {
  border: none;
}

