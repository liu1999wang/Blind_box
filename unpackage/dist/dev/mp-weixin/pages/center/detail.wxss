page {
  background: url(data:image/png;base64,);
  background-size: 100% auto;
  background-repeat: no-repeat;
  background-color: #f5f8fc;
}
.container {
  padding-top: 60px;
}
.header {
  display: flex;
  align-items: center;
}
.header .headimg {
  width: 160rpx;
  flex: 0 0 160rpx;
  margin: 0rpx 30rpx;
  height: 160rpx;
  border-radius: 50%;
  border: 10rpx solid white;
  box-shadow: 0px 14px 30px 0px rgba(226, 222, 204, 0.4);
}
.header .middle {
  display: flex;
  flex-direction: column;
}
.header .middle .name {
  font-size: 34rpx;
  font-weight: bold;
  color: #111111;
}
.header .middle .vip-c {
  margin-top: 20rpx;
  padding: 0rpx 20rpx;
  border-radius: 50rpx;
  display: flex;
  align-items: center;
  background: #FFF8DD;
  font-size: 24rpx;
  font-weight: 500;
  color: #FFCC09;
}
.header .middle .vip-c .icon-vip {
  margin-right: 10rpx;
  font-size: 36rpx;
  line-height: 40rpx;
}
.header .middle .vip-c.disabled {
  color: #999999;
}
.header .right {
  margin-right: 30rpx;
  padding: 0rpx;
  font-size: 26rpx;
  width: 130rpx;
  border-radius: 100rpx !important;
  line-height: 50rpx;
  overflow: hidden;
  height: 50rpx;
  color: black;
  background-color: #FFF8DD;
}
.header .right::after {
  display: none;
}
.status-bar {
  display: flex;
  margin: 50rpx 0rpx 30rpx 0rpx;
  align-items: center;
}
.status-bar .line {
  height: 40rpx;
  width: 1rpx;
  flex: 0 0 1rpx;
  border-left: 1rpx solid #999;
}
.status-bar .item {
  flex-grow: 1;
  text-align: center;
}
.status-bar .item .number {
  color: black;
  font-size: 32rpx;
  font-weight: 700;
  line-height: 40rpx;
}
.status-bar .item .text {
  font-size: 24rpx;
  font-weight: 500;
  color: #FFFFFF;
}
.order-c {
  width: 700rpx;
  height: 200rpx;
  background: #FFFFFF;
  box-shadow: 0px 14rpx 30rpx 0px rgba(243, 238, 220, 0.4);
  border-radius: 20rpx;
  margin: 0rpx auto;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}
.order-c .top {
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
}
.order-c .top .title {
  font-weight: bold;
  color: #333333;
  font-size: 28rpx;
  flex-grow: 1;
}
.order-c .top .all-btn {
  font-size: 24rpx;
  font-weight: 500;
  color: #FFCC09;
}
.order-c .body {
  display: flex;
  align-items: flex-end;
  width: 100%;
  justify-content: space-between;
}
.order-c .body .item {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.order-c .body .item text {
  font-weight: bold;
  color: #333333;
  font-size: 24rpx;
}
.share-banner {
  width: 700rpx;
  height: 218rpx;
  display: block;
  margin: 20rpx auto 0rpx auto;
}
.action-list {
  width: 700rpx;
  margin: 0rpx auto 20rpx auto;
  background: #FFFFFF;
  box-shadow: 0px 14rpx 30rpx 0px rgba(243, 238, 220, 0.16);
  border-radius: 20rpx;
  position: relative;
  top: -50rpx;
}
.action-list .item {
  display: flex;
  align-items: center;
  padding: 30rpx 20rpx 30rpx 30rpx;
  border-bottom: 1rpx solid #F3F3F3;
}
.action-list .item .left-icon {
  width: 60rpx;
  flex: 0 0 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.action-list .item .left-icon image {
  width: 34rpx;
  height: 34rpx;
}
.action-list .item .text {
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
  flex-grow: 1;
}
.action-list .item .total {
  height: 40rpx;
  background: #F2F2F2;
  border-radius: 20rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
  padding: 0rpx 30rpx;
  margin-right: 10rpx;
}
.action-list .item .icon-arrow-right {
  display: flex;
  align-items: center;
  font-size: 42rpx;
  display: block;
  color: #aaa;
  line-height: 42rpx;
  padding: 0rpx;
}
.float-btn {
  width: 160rpx;
  height: 160rpx;
  position: fixed;
  right: 10rpx;
  bottom: 160rpx;
}
@-webkit-keyframes yaoyiyao {
0% {
    -webkit-transform: scale(1) rotate(-4deg);
            transform: scale(1) rotate(-4deg);
}
25% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
50% {
    -webkit-transform: scale(1.1) rotate(4deg);
            transform: scale(1.1) rotate(4deg);
}
75% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: scale(1) rotate(-4deg);
            transform: scale(1) rotate(-4deg);
}
}
@keyframes yaoyiyao {
0% {
    -webkit-transform: scale(1) rotate(-4deg);
            transform: scale(1) rotate(-4deg);
}
25% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
50% {
    -webkit-transform: scale(1.1) rotate(4deg);
            transform: scale(1.1) rotate(4deg);
}
75% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
100% {
    -webkit-transform: scale(1) rotate(-4deg);
            transform: scale(1) rotate(-4deg);
}
}

