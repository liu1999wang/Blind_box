.yzm_body {
  background: rgba(0, 0, 0, 0.3);
  position: fixed;
  top: 0;
  z-index: 999999;
  width: 750rpx;
  height: 100vh;
}
.yzm_body .zhuti {
  background: #FFF;
  width: 600rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  padding: 20rpx 25rpx 30rpx 25rpx;
  border-radius: 15rpx;
}
.yzm_body .zhuti .msg {
  color: #999;
  font-size: 24rpx;
}
.yzm_body .zhuti .msg.red {
  color: red;
}
.yzm_body .zhuti .msg.green {
  color: green;
}
.yzm_body .zhuti .close {
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  color: #A6A6A6;
  font-size: 36rpx;
  padding: 10rpx;
}
.yzm_body .zhuti .title {
  font-size: 28rpx;
  line-height: 38rpx;
  color: #333;
  margin: 0 0 7rpx 0;
}
.yzm_body .zhuti movable-area {
  width: 100%;
  height: auto;
}
.yzm_body .zhuti movable-area #msg {
  width: 100%;
  line-height: 310rpx;
  text-align: center;
  font-size: 30rpx;
  color: #999;
  background: #F8F8F8;
}
.yzm_body .zhuti movable-area .pic {
  width: 100%;
  height: 310rpx;
  background-color: #F8F8F8;
  background-size: 100% 310rpx;
}
.yzm_body .zhuti movable-area #line {
  background: #e4e4e4;
  width: 100%;
  height: 20rpx;
  margin: 42rpx 0 15rpx 0;
  border-radius: 50rpx;
  display: inline-block;
}
.yzm_body .zhuti movable-area movable-view {
  width: 72rpx;
  height: 310rpx;
  background-size: 100% 310rpx;
}
.yzm_body .zhuti movable-area movable-view .blue {
  box-shadow: rgba(26, 101, 255, 0.52) 0px 0px 10px 1px;
  width: 100rpx;
  height: 50rpx;
  border-radius: 50rpx;
  position: absolute;
  top: 100%;
  left: 0;
  background: #1a65ff url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACgAAAAcAgMAAABuexVFAAAACVBMVEUAAADCwsL9/f1P0DqbAAAAAXRSTlMAQObYZgAAAB1JREFUGNNjCGVgYGANABKhyMwoEHMBkIgaZWIwAdyJJQnaJRg5AAAAAElFTkSuQmCC) no-repeat;
  background-size: auto 20rpx;
  background-position: 50% 50%;
  margin: 25rpx 0 0 -14rpx;
}

