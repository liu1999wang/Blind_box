<block wx:if="{{isInit}}"><view class="container data-v-86fb6ef8"><view class="day-c data-v-86fb6ef8"><block wx:for="{{days}}" wx:for-item="item" wx:for-index="n"><view class="item data-v-86fb6ef8"><view class="{{['round','data-v-86fb6ef8',(days[n].is_completed)?'completed':'']}}"><text class="new-iconfont icon-right data-v-86fb6ef8"></text></view><view class="score data-v-86fb6ef8">{{"+"+days[n].score}}</view><view class="day data-v-86fb6ef8">{{"第"+days[n].number+"天"}}</view></view></block></view><block wx:if="{{!isSignIn}}"><view class="signin-btn data-v-86fb6ef8" hover-class="hover" data-event-opts="{{[['tap',[['signIn',['$event']]]]]}}" bindtap="__e">签到</view></block><block wx:else><view class="signin-btn completed data-v-86fb6ef8">今天已签到</view></block><view class="signin-total data-v-86fb6ef8">{{"累计签到"+signinTotal+"天"}}</view></view></block>